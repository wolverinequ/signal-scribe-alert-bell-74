    {
      "src": "/placeholder.svg", 
      "sizes": "512x512",
      "type": "image/svg+xml"
    }
  ],
  "permissions": [
    "notifications",
    "vibrate",
    "wake-lock"
  ],
  "prefer_related_applications": false
}
