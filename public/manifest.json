
{
  "name": "Binary Options Signal Tracker",
  "short_name": "SignalTracker",
  "description": "Track and get notifications for binary options signals",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/placeholder.svg",
      "sizes": "192x192",
